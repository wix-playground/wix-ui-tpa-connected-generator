import React from 'react'
import styles from './<%=componentName%>.st.css'
import {<%=componentName%>} from '<%=componentModuleName%>/<%=componentName%>'

export const <%=componentName%>Ext = (props) => {
  <% if (namespace) { %>
    const namespace = '<%=namespace%>'
    props = {...props}
    props.className = props.className ? `${props.className} ${namespace}` : namespace
  <% } %>
  return React.createElement(<%=componentName%>, {...props, ...styles('root', {}, props)})
}
