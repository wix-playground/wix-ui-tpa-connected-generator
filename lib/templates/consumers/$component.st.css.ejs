:import {
  -st-from: "wix-ui-tpa/dist/src/components/<%=componentName%>/<%=componentName%>.st.css";
  -st-default: TPAMixin;
}

.root {
  -st-mixin: TPAMixin(
    <% Object.entries(variables).forEach(([name, value], index) => { %>
      <%=name%> <%=value%><%=(index + 1 < Object.keys(variables).length ? ',' : '')%>
    <% }) %>
  );
}
